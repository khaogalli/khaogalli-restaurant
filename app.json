{
  "expo": {
    "name": "KhaoGalli Restaurant",
    "slug": "KhaoGalli",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/download.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/download.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffce4a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/download.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.github.khaogalli.khaogallirestaurant"
    },
    "web": {
      "favicon": "./assets/download.png"
    },
    "extra": {
      "eas": {
        "projectId": "29c21d74-a49d-47af-a977-9341250c772a"
      }
    },
    "plugins": [
      ["expo-build-properties", {
        "android": {
          "usesCleartextTraffic": true
        }
      }]
    ]
  }
}
